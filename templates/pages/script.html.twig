{% extends 'base.html.twig' %}

{% block title %} Scripts {% endblock %}

{% block body %}

    <table class="table table-striped">
        <thead>
            <tr>
                <th> Title </th>
                <th> Company </th>
                <th> Character Name </th>
                <th> Actor Pay </th>
                <th> Actor Revenue </th>
                <th> Lines Per Script </th>
                <th> Words Per Script </th>
                <th> Actor Mentions</th>
                <th> Company Revenue </th>
                <th> Movies Per Year</th>
                <th> Percent Of Fails </th>
                <th> Losses </th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>{{ScriptTitles.title}}</td>
                <td>{{ScriptTitles.company}}</td>
                <td>{{ScriptTitles.actorName}}</td>
                <td>{{ScriptTitles.actorPay}}</td>
                <td>{{ScriptTitles.actorRevenue}}</td>
                <td>{{ScriptTitles.linesPerActor}}</td>
                <td>{{ScriptTitles.wordsPerActor}}</td>
                <td>{{ScriptTitles.mentionsPerActor}}</td>
                <td>{{ScriptTitles.companyRevenue}}</td>
                <td>{{ScriptTitles.moviesPerYear}}</td>
                <td>{{ScriptTitles.percentOfFails}}</td>
                <td>{{ScriptTitles.losses}}</td>
            </tr>
    </table>
    <a href="/scripts/">Go Back</a>

{% endblock %} 